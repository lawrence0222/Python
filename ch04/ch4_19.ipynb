   "source": [
    "# 計算出平均值"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "請輸入三個數字： 17,28,59\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "三個數字的平均是： 34.67\n"
     ]
    }
   ],
   "source": [
    "n1, n2, n3 = eval(input(\"請輸入三個數字：\"))\n",
    "average = (n1 + n2 + n3) / 3\n",
    "print(\"三個數字的平均是：%6.2f\" % average)"
   ]
  },
