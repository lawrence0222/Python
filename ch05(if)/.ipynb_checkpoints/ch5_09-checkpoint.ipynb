     "text": [
      "請輸入身高(公分)： 172\n",
      "請輸入體重(公斤)： 71\n"
     ]
    }
   ],
   "source": [
    "height = eval(input(\"請輸入身高(公分)：\"))\n",
    "weight = eval(input(\"請輸入體重(公斤)：\"))\n",
    "bmi = weight / (height / 100) ** 2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
