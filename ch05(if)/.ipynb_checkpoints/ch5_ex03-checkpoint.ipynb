   "source": [
    "# 圓半徑20，圓心在(0,0)，輸入任意點座標，判斷此點是否在圓內"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "輸入任意點座標： 18, 20\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "此座標在圓外\n"
     ]
    }
   ],
   "source": [
    "x, y = eval(input(\"輸入任意點座標：\"))\n",
    "\n",
    "dist = (x ** 2 + y ** 2) ** 0.5\n",
    "\n",
    "if dist <= 20:\n",
    "    print(\"此座標在圓內\")\n",
    "else:\n",
    "    print(\"此座標在圓外\")"
   ]
  }
 ],
