    "e = 1\n",
    "for n in range(1,101):\n",
    "    temp = 1\n",
    "    for i in range(1, n+1):\n",
    "        temp *= i\n",
    "    e += (1 / temp)\n",
    "    if n % 10 == 0:\n",
    "        print(\"第%d次計算，e = %.50f\" % (n, e))"
   ]
