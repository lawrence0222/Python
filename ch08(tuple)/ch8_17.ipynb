   "source": [
    "# 如果放在zip()內的串列參數長度不相等，多出的元素無法匹配，所以轉成zip物件後將是較短的數量"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "fields = ['Name', 'Age0', 'Hometown']\n",
    "info = ['Peter', '30']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "zipdata = zip(fields, info)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "player = list(zipdata)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[('Name', 'Peter'), ('Age0', '30')]\n"
     ]
    }
   ],
   "source": [
    "print(player)"
   ]
  }
 ],
