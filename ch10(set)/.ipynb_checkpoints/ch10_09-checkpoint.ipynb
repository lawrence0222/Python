   "source": [
    "# 大數據資料與集合的應用\n",
    "使用set()函數，就能快速的將重複資料排除"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "fruits1 = ['apple', 'banana', 'apple', 'orange', 'banana']\n",
    "x = set(fruits1)\n",
    "fruits2 = list(x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "原先串列fruits1資料 =  ['apple', 'banana', 'apple', 'orange', 'banana']\n",
      "新的串列fruits2資料 =  ['apple', 'banana', 'orange']\n"
     ]
    }
   ],
   "source": [
    "print(\"原先串列fruits1資料 = \", fruits1)\n",
    "print(\"新的串列fruits2資料 = \", fruits2)"
   ]
  }
 ],
