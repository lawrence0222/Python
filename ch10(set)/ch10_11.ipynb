   "source": [
    "# 使用intersection()方法完成交集的應用"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "A = {1, 2, 3, 4, 5}\n",
    "B = {3, 4, 5, 6, 7}\n",
    "AB = A.intersection(B)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "A和B的交集是  {3, 4, 5}\n"
     ]
    }
   ],
   "source": [
    "print(\"A和B的交集是 \", AB)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "B和A的交集是  {3, 4, 5}\n"
     ]
    }
   ],
   "source": [
    "BA = B.intersection(A)\n",
    "print(\"B和A的交集是 \", BA)"
   ]
  }
