   "source": [
    "print(\"● ax + by = e \\n\" + \\\n",
    "      \"● cx + dy = f \\n\")\n",
    "a, b, c, d, e, f = eval(input(\"請依序給予a~f值(用逗號隔開)：\"))\n",
    "\n",
    "if a * d - b * c == 0:\n",
