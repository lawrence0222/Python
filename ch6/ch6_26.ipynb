   "source": [
    "cars = ['BMW', 'Honda', 'Nissan', 'Toyota', 'Ford']\n",
    "print(\"目前串列內容：\", cars)\n",
    "popped_car = cars.pop()\n",
    "print(\"使用pop()所刪除的元素是%s\" % popped_car)\n",
    "print(\"目前串列內容：\", cars)\n",
    "popped_car = cars.pop(1)\n",
    "print(\"使用pop(1)所刪除的元素是%s\" % popped_car)\n",
    "print(\"目前串列內容：\", cars)"
   ]
  }
