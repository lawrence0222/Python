   "source": [
    "x1 = eval(input(\"請輸入第一個數字：\"))\n",
    "x2 = eval(input(\"請輸入第二個數字：\"))\n",
    "n = 2\n",
    "gcd = 0\n",
    "while True:\n",
    "    if x1 % n == 0 and x2 % n == 0:\n",
    "        gcd = n\n",
    "    elif n == x1 or n == x2:\n",
    "        break\n",
    "    n += 1\n",
    "\n",
    "print(\"%d和%d的最大公因數%d\" % (x1, x2, gcd))"
   ]
