   "source": [
    "check = True\n",
    "while check:\n",
    "    n = eval(input(\"請輸入n值：\"))\n",
    "    m = eval(input(\"請輸入m值：\"))\n",
    "    if m <= n:\n",
    "        print(\"m值必須大於n值，請重新輸入\")\n",
    "        continue\n",
    "    check = False\n",
    "sum = 0\n",
    "for n in range(n, m+1):\n",
    "    sum += n\n",
    "print(\"%d加到%d的結果是：%d\" % (n, m, sum))"
   ]
  }
 ],
