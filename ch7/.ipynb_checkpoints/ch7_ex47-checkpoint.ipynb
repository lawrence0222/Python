   "source": [
    "x = 1000001\n",
    "pi = 0\n",
    "for i in range(1,x+1):\n",
    "    pi += 4 * ((-1) ** (i + 1) / (2 * i - 1))\n",
    "    if i % 100000 == 0:\n",
    "        print(\"當 i = %7d 時，PI = %20.19f\" % (i, pi))"
   ]
  }
